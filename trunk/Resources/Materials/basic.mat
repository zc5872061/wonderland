[shaders]

vertex=Shaders/vertex.fx
fragment=Shaders/fragment.fx

[engine]

u_lightDirection=map:'lightdirection';size:3;
u_cameraPos=map:'cameraposition';size:3;
u_mvpMatrix=map:'modelviewprojection';size:'matrix';
u_mMatrix=map:'model';size:'matrix';

[config]

u_ambientColor=size:4;
u_diffuseColor=size:4;
u_fresnelAmount=size:1;

[input]

a_normal=map:'normal';size:3;
a_position=map:'position';size:4;

